{
    "name": "weble/laravelshoppingcart",
    "description": "Laravel Shoppingcart - Fork of gloudemans/shoppingcart",
    "keywords": ["laravel", "shoppingcart", "cart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Gloudemans",
            "email": "info@robgloudemans.nl"
        },
        {
            "name": "Daniele Rosario",
            "email": "daniele@weble.it"
        }
    ],
    "require": {
        "illuminate/support": "5.6.* || 5.7.* || 5.8.*",
        "illuminate/session": "5.6.* || 5.7.* || 5.8.*",
        "illuminate/events": "5.6.* || 5.7.* || 5.8.*",
        "cknow/laravel-money": "^3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0 || ~8.0",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Weble\\LaravelShoppingCart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Weble\\Tests\\LaravelShoppingCart\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Weble\\LaravelShoppingCart\\ShoppingCartServiceProvider"
            ],
            "aliases": {
                "Cart": "Weble\\LaravelShoppingCart\\Facades\\Cart"
            }
        }
    }
}
